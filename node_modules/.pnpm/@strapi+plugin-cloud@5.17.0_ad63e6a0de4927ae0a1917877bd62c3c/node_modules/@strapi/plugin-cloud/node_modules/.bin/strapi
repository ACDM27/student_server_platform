#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/@strapi+strapi@5.17.0_@babe_11d094e00ece21d4e34f7045952b0ddf/node_modules/@strapi/strapi/bin/node_modules:/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/@strapi+strapi@5.17.0_@babe_11d094e00ece21d4e34f7045952b0ddf/node_modules/@strapi/strapi/node_modules:/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/@strapi+strapi@5.17.0_@babe_11d094e00ece21d4e34f7045952b0ddf/node_modules/@strapi/node_modules:/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/@strapi+strapi@5.17.0_@babe_11d094e00ece21d4e34f7045952b0ddf/node_modules:/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/@strapi+strapi@5.17.0_@babe_11d094e00ece21d4e34f7045952b0ddf/node_modules/@strapi/strapi/bin/node_modules:/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/@strapi+strapi@5.17.0_@babe_11d094e00ece21d4e34f7045952b0ddf/node_modules/@strapi/strapi/node_modules:/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/@strapi+strapi@5.17.0_@babe_11d094e00ece21d4e34f7045952b0ddf/node_modules/@strapi/node_modules:/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/@strapi+strapi@5.17.0_@babe_11d094e00ece21d4e34f7045952b0ddf/node_modules:/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@strapi+strapi@5.17.0_@babe_11d094e00ece21d4e34f7045952b0ddf/node_modules/@strapi/strapi/bin/strapi.js" "$@"
else
  exec node  "$basedir/../../../../../../@strapi+strapi@5.17.0_@babe_11d094e00ece21d4e34f7045952b0ddf/node_modules/@strapi/strapi/bin/strapi.js" "$@"
fi
