#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/@strapi+pack-up@5.1.0_@swc+_f09c52e05d2b0a70dad3b15b84b94fc4/node_modules/@strapi/pack-up/bin/node_modules:/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/@strapi+pack-up@5.1.0_@swc+_f09c52e05d2b0a70dad3b15b84b94fc4/node_modules/@strapi/pack-up/node_modules:/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/@strapi+pack-up@5.1.0_@swc+_f09c52e05d2b0a70dad3b15b84b94fc4/node_modules/@strapi/node_modules:/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/@strapi+pack-up@5.1.0_@swc+_f09c52e05d2b0a70dad3b15b84b94fc4/node_modules:/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/@strapi+pack-up@5.1.0_@swc+_f09c52e05d2b0a70dad3b15b84b94fc4/node_modules/@strapi/pack-up/bin/node_modules:/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/@strapi+pack-up@5.1.0_@swc+_f09c52e05d2b0a70dad3b15b84b94fc4/node_modules/@strapi/pack-up/node_modules:/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/@strapi+pack-up@5.1.0_@swc+_f09c52e05d2b0a70dad3b15b84b94fc4/node_modules/@strapi/node_modules:/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/@strapi+pack-up@5.1.0_@swc+_f09c52e05d2b0a70dad3b15b84b94fc4/node_modules:/mnt/d/strapi_project/studentplatform/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@strapi/pack-up/bin/pack-up.js" "$@"
else
  exec node  "$basedir/../@strapi/pack-up/bin/pack-up.js" "$@"
fi
