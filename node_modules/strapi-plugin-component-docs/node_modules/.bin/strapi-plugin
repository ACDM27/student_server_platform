#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/student_platform_server/node_modules/.pnpm/@strapi+sdk-plugin@5.3.2_@s_4d153ecb2806099c33051123d6e59fb0/node_modules/@strapi/sdk-plugin/bin/node_modules:/mnt/d/student_platform_server/node_modules/.pnpm/@strapi+sdk-plugin@5.3.2_@s_4d153ecb2806099c33051123d6e59fb0/node_modules/@strapi/sdk-plugin/node_modules:/mnt/d/student_platform_server/node_modules/.pnpm/@strapi+sdk-plugin@5.3.2_@s_4d153ecb2806099c33051123d6e59fb0/node_modules/@strapi/node_modules:/mnt/d/student_platform_server/node_modules/.pnpm/@strapi+sdk-plugin@5.3.2_@s_4d153ecb2806099c33051123d6e59fb0/node_modules:/mnt/d/student_platform_server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/student_platform_server/node_modules/.pnpm/@strapi+sdk-plugin@5.3.2_@s_4d153ecb2806099c33051123d6e59fb0/node_modules/@strapi/sdk-plugin/bin/node_modules:/mnt/d/student_platform_server/node_modules/.pnpm/@strapi+sdk-plugin@5.3.2_@s_4d153ecb2806099c33051123d6e59fb0/node_modules/@strapi/sdk-plugin/node_modules:/mnt/d/student_platform_server/node_modules/.pnpm/@strapi+sdk-plugin@5.3.2_@s_4d153ecb2806099c33051123d6e59fb0/node_modules/@strapi/node_modules:/mnt/d/student_platform_server/node_modules/.pnpm/@strapi+sdk-plugin@5.3.2_@s_4d153ecb2806099c33051123d6e59fb0/node_modules:/mnt/d/student_platform_server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@strapi+sdk-plugin@5.3.2_@s_4d153ecb2806099c33051123d6e59fb0/node_modules/@strapi/sdk-plugin/bin/strapi-plugin.js" "$@"
else
  exec node  "$basedir/../../../../../@strapi+sdk-plugin@5.3.2_@s_4d153ecb2806099c33051123d6e59fb0/node_modules/@strapi/sdk-plugin/bin/strapi-plugin.js" "$@"
fi
